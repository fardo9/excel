{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parents": "off",
        "eol-last": ["error", "never"],
        "no-multiple-empty-lines": [2, {"max": 99999, "maxEOF": 0}]
    },
    "extends": ["eslint:recommended", "google"]
}